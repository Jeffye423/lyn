{
  "version": 3,
  "builds": [
    {
      "src": "dist/**/*",
      "use": "@vercel/static",
      "config": { "zeroConfig": true }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/dist/$1",
      "check": true
    },
    {
      "src": "/.*",
      "status": 404,
      "dest": "/dist/404.html"
    }
  ]
}
